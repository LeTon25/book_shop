@model List<ApplicationUser>
<div class="container">
    <div class="row pt-4">
        <div class="col-6">
            <h2>Danh sách các người dùng</h2>
        </div>
        <div class="col-6 text-end">
            <a class="btn btn-success" asp-area="Admin" asp-controller="Product" asp-action="Upsert">
                <i class="bi bi-plus-circle"></i>
                Tạo mới
            </a>
        </div>
    </div>
</div>
<table id="tblData" class="table table-striped table-hover">
    <thead>
        <tr>
            <th>Tên </th>
            <th>Email</th>
            <th>Số điện thoại</th>
            <th>Vai trò</th>
            <th>Hành động</th>
        </tr>
    </thead>
</table>
@section Scripts{
    <script src="~/js/user.js"></script>
}
